<?xml version="1.0"?>
<ruleset xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="Oci PSR-12" xsi:noNamespaceSchemaLocation="https://raw.githubusercontent.com/squizlabs/PHP_CodeSniffer/master/phpcs.xsd">

        <rule ref="PSR12">
            <exclude name="Generic.Files.LineLength" />
            <exclude name="PSR12.Files.FileHeader" />
            <exclude name="PSR12.Files.OpenTag" />
            <exclude-pattern>*.js</exclude-pattern>
        </rule>
</ruleset>
